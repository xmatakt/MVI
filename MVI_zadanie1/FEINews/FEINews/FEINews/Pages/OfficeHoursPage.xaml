<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FEINews.OfficeHoursPage"
             Title="Otváracie hodiny objektov">
    <ContentPage.Content>
        <StackLayout>
            <ListView x:Name="OfficeHoursListView">
                <ListView.ItemTemplate>
                    <DataTemplate>
                        <TextCell TextColor="Gray" Text="{Binding Name}" Tapped="TextCell_Tapped"/>
                    </DataTemplate>
                </ListView.ItemTemplate>
            </ListView>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>